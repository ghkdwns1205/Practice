package Codingtest;

import java.util.Scanner;

public class Coding_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = 0;
		for(int i = 0; i<=a; i++) {
		 b += a;
		}
		System.out.println(b);
	}

}
